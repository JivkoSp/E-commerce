@page
@model BooksPlace.Pages.Register.SignUpModel

    <form method="post">
        <label for="UserName">Username or email address </label>
        <input id="UserName" type="text" asp-for="UserName"/> <br />
        <label for="Email">Email address </label>
        <input id="Email" type="email" asp-for="Email" /> <br />
        <label for="Password">Password </label>
        <input id="Password" type="password" asp-for="Password" /><br />
        <button type="submit">Sign up</button>
    </form>
